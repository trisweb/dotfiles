[color]
	 branch = auto
	 diff = auto
	 status = auto
[color "branch"]
	 current = yellow reverse
	 local = yellow
	 remote = green
[color "diff"]
	 meta = yellow bold
	 frag = magenta bold
	 old = red
	 new = green
[color "status"]
	 added = yellow
	 changed = green
	 untracked = cyan
